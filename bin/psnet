#!/usr/bin/env python3

import yaml
import uvicorn
from northbound.psnet import psnet

if __name__ == "__main__":
    with open("config.yaml", "r") as config_yaml:
        config = yaml.safe_load(config_yaml).get("psnet", {})

    uvicorn.run(psnet, host=config.get("host"), port=config.get("port"))
